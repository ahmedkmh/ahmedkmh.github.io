u064a\u062e\u0636 \u0645\u0648\u0627\u062c\u0647\u0627\u062a \u0631\u0633\u0645\u064a\u0629 \u0645\u0646\u062a\u0638\u0645\u0629 \u0627\u0654\u0645\u0627\u0645 \u0627\u0644\u0627\u0654\u0646\u062f\u064a\u0629 \u0627\u0644\u0627\u0654\u0648\u0631\u0648\u0628\u064a\u0629\u060c \u0627\u0655\u0644\u0627 \u0627\u0654\u0646 \u062a\u0627\u0631\u064a\u062e\u0647 \u0641\u064a \u0627\u0644\u0644\u0642\u0627\u0621\u0627\u062a \u0627\u0644\u0648\u062f\u064a\u0629 \u064a\u0639\u0643\u0633 \u0627\u0654\u062f\u0627\u0621\u064b \u0645\u0634\u0631\u0641\u064b\u0627 \u064a\u064f\u0628\u0631\u0632 \u0642\u0648\u0629 \u0627\u0644\u0641\u0631\u064a\u0642 \u0648\u0627\u0633\u062a\u0639\u062f\u0627\u062f\u0647 \u0644\u0644\u0638\u0647\u0648\u0631 \u0628\u0645\u0633\u062a\u0648\u0649 \u064a\u0644\u064a\u0642 \u0628\u0647 \u0639\u0627\u0644\u0645\u064a\u064b\u0627.\u0627\u0644\u0647\u0644\u0627\u0644 \u0628\u062f\u0627\u0654 \u0645\u0634\u0648\u0627\u0631\u0647 \u0641\u064a \u0643\u0627\u0654\u0633 \u0627\u0644\u0639\u0627\u0644\u0645 \u0644\u0644\u0627\u0654\u0646\u062f\u064a\u0629 2025 \u0628\u062a\u0639\u0627\u062f\u0644 \u062a\u0627\u0631\u064a\u062e\u064a \u0627\u0654\u0645\u0627\u0645 \u0631\u064a\u0627\u0644 \u0645\u062f\u0631\u064a\u062f \u0627\u0644\u0627\u0655\u0633\u0628\u0627\u0646\u064a\u060c \u0644\u064a\u0635\u0628\u062d \u0627\u0654\u0648\u0644 \u0646\u0627\u062f\u064d \u0639\u0631\u0628\u064a \u0648\u0627\u0653\u0633\u064a\u0648\u064a \u064a\u062a\u0645\u0643\u0646 \u0645\u0646 \u062d\u0635\u062f \u0646\u0642\u0637\u0629 \u0627\u0654\u0645\u0627\u0645 \u0627\u0644\u0639\u0645\u0644\u0627\u0642 \u0627\u0644\u0627\u0655\u0633\u0628\u0627\u0646\u064a.\u0627\u0654\u0648\u0644\u0649 \u0645\u0648\u0627\u062c\u0647\u0627\u062a \u0627\u0644\u0647\u0644\u0627\u0644 \u0627\u0654\u0645\u0627\u0645 \u0627\u0644\u0641\u0631\u0642 \u0627\u0644\u0627\u0654\u0648\u0631\u0648\u0628\u064a\u0629 \u062c\u0627\u0621\u062a \u0639\u0627\u0645 1984\u060c \u062d\u064a\u0646 \u0648\u0627\u062c\u0647 \u0646\u0648\u062a\u0646\u062c\u0647\u0627\u0645 \u0641\u0648\u0631\u0633\u062a \u0641\u064a \u0645\u0628\u0627\u0631\u0627\u0629 \u0648\u062f\u064a\u0629\u060c \u0648\u062e\u0633\u0631 \u0628\u0646\u062a\u064a\u062c\u0629 8-0\u060c \u0648\u0643\u0627\u0646 \u0646\u0648\u062a\u0646\u062c\u0647\u0627\u0645 \u0641\u064a \u0630\u0644\u0643 \u0627\u0644\u0648\u0642\u062a \u0627\u0654\u062d\u062f \u0627\u0654\u0628\u0631\u0632 \u0627\u0644\u0627\u0654\u0646\u062f\u064a\u0629 \u0627\u0644\u0627\u0655\u0646\u062c\u0644\u064a\u0632\u064a\u0629.\u062a\u0641\u0648\u0642 \u0627\u0644\u0647\u0644\u0627\u0644 \u0639\u0644\u0649 \u0627\u0644\u0627\u0654\u0646\u062f\u064a\u0629 \u0627\u0644\u0643\u0628\u0631\u0649: \u0627\u0644\u0628\u062f\u0627\u064a\u0629 \u0645\u0646 \u0641\u0627\u0644\u0646\u0633\u064a\u0627\u0641\u064a \u0639\u0627\u0645 2005\u060c \u0648\u0627\u062c\u0647 \u0627\u0644\u0647\u0644\u0627\u0644 \u0646\u0627\u062f\u064a \u0641\u0627\u0644\u0646\u0633\u064a\u0627 \u0627\u0644\u0627\u0655\u0633\u0628\u0627\u0646\u064a \u0641\u064a \u0645\u0628\u0627\u0631\u0627\u0629 \u0627\u0639\u062a\u0632\u0627\u0644 \u0627\u0644\u0646\u062c\u0645 \u064a\u0648\u0633\u0641 \u0627\u0644\u062b\u0646\u064a\u0627\u0646. \u0648\u0631\u063a\u0645 \u0643\u0648\u0646 \u0627\u0644\u0644\u0642\u0627\u0621 \u0648\u062f\u064a\u064b\u0627 \u0648\u062a\u0643\u0631\u064a\u0645\u064a\u064b\u0627\u060c \u0627\u0655\u0644\u0627 \u0627\u0654\u0646 \u0627\u0644\u0647\u0644\u0627\u0644 \u0627\u0654\u0638\u0647\u0631 \u0627\u0654\u062f\u0627\u0621\u064b \u0645\u0645\u064a\u0632\u064b\u0627\u060c \u0648\u062d\u0642\u0642 \u0627\u0644\u0641\u0648\u0632 \u0628\u0646\u062a\u064a\u062c\u0629 2-1\u060c \u0641\u064a \u0627\u0654\u0645\u0633\u064a\u0629 \u0643\u0631\u0648\u064a\u0629 \u062d\u0638\u064a\u062a \u0628\u0627\u0655\u0634\u0627\u062f\u0629 \u0648\u0627\u0633\u0639\u0629.\u0627\u0646\u062a\u0635\u0627\u0631 \u0639\u0644\u0649 \u0645\u0627\u0646\u0634\u0633\u062a\u0631 \u064a\u0648\u0646\u0627\u064a\u062a\u062f: \u0644\u064a\u0644\u0629 \u062a\u0627\u0631\u064a\u062e\u064a\u0629 \u0641\u064a \u0627\u0644\u0631\u064a\u0627\u0636\u0641\u064a \u0639\u0627\u0645 2008\u060c \u0639\u0627\u062f \u0627\u0644\u0647\u0644\u0627\u0644 \u0644\u064a\u062d\u0642\u0642 \u0641\u0648\u0632\u064b\u0627 \u0644\u0627\u0641\u062a\u064b\u0627 \u0639\u0644\u0649 \u0645\u0627\u0646\u0634\u0633\u062a\u0631 \u064a\u0648\u0646\u0627\u064a\u062a\u062f \u0627\u0644\u0627\u0655\u0646\u062c\u0644\u064a\u0632\u064a\u060c \u0628\u0637\u0644 \u062f\u0648\u0631\u064a \u0627\u0654\u0628\u0637\u0627\u0644 \u0627\u0654\u0648\u0631\u0648\u0628\u0627 \u0641\u064a \u062a\u0644\u0643 \u0627\u0644\u0633\u0646\u0629\u060c \u0648\u0630\u0644\u0643 \u0641\u064a \u0645\u0628\u0627\u0631\u0627\u0629 \u0627\u0639\u062a\u0632\u0627\u0644 \u0633\u0627\u0645\u064a \u0627\u0644\u062c\u0627\u0628\u0631. \u0627\u0644\u0644\u0642\u0627\u0621\u060c \u0627\u0644\u0630\u064a \u0634\u0647\u062f \u062d\u0636\u0648\u0631\u064b\u0627 \u062c\u0645\u0627\u0647\u064a\u0631\u064a\u064b\u0627 \u0643\u0628\u064a\u0631\u064b\u0627\u060c \u0627\u0646\u062a\u0647\u0649 \u0628\u0641\u0648\u0632 \u0627\u0644\u0647\u0644\u0627\u0644 3-2\u060c \u0641\u064a \u0648\u0627\u062d\u062f\u0629 \u0645\u0646 \u0627\u0654\u0628\u0631\u0632 \u0627\u0644\u0645\u062d\u0637\u0627\u062a \u0627\u0644\u062a\u0627\u0631\u064a\u062e\u064a\u0629 \u0644\u0644\u0641\u0631\u064a\u0642 \u0627\u0654\u0645\u0627\u0645 \u0646\u0627\u062f\u064d \u0627\u0654\u0648\u0631\u0648\u0628\u064a \u0643\u0628\u064a\u0631.\u0639\u0648\u062f\u0629 \u0627\u0655\u0644\u0649 \u0627\u0654\u0648\u0631\u0648\u0628\u0627: \u0645\u0648\u0627\u062c\u0647\u0629 \u0627\u0654\u0648\u062f\u064a\u0646\u064a\u0632\u064a \u0641\u064a 2019\u0628\u0639\u062f \u0627\u0654\u0643\u062b\u0631 \u0645\u0646 \u0639\u0642\u062f \u0639\u0644\u0649 \u0644\u0642\u0627\u0621 \u0645\u0627\u0646\u0634\u0633\u062a\u0631 \u064a\u0648\u0646\u0627\u064a\u062a\u062f\u060c \u0627\u0644\u062a\u0642\u0649 \u0627\u0644\u0647\u0644\u0627\u0644 \u0646\u0627\u062f\u064a \u0627\u0654\u0648\u062f\u064a\u0646\u064a\u0632\u064a \u0627\u0644\u0627\u0655\u064a\u0637\u0627\u0644\u064a \u0641\u064a \u0645\u0628\u0627\u0631\u0627\u0629 \u0648\u062f\u064a\u0629 \u0639\u0627\u0645 2019 \u0636\u0645\u0646 \u0627\u0633\u062a\u0639\u062f\u0627\u062f\u0627\u062a\u0647 \u0644\u0644\u0645\u0648\u0633\u0645 \u0627\u0644\u062c\u062f\u064a\u062f\u060c \u0648\u0627\u0646\u062a\u0647\u062a \u0628\u0641\u0648\u0632 \u0627\u0644\u0647\u0644\u0627\u0644 3-2\u060c \u0641\u064a \u0644\u0642\u0627\u0621 \u0627\u0654\u0643\u062f \u0627\u0633\u062a\u0645\u0631\u0627\u0631 \u062a\u0637\u0648\u0631 \u0627\u0644\u0641\u0631\u064a\u0642 \u0628\u0639\u062f \u062a\u062a\u0648\u064a\u062c\u0647 \u0628\u062f\u0648\u0631\u064a \u0627\u0654\u0628\u0637\u0627\u0644 \u0627\u0653\u0633\u064a\u0627 \u0641\u064a \u0646\u0641\u0633 \u0627\u0644\u0639\u0627\u0645.\u0645\u0628\u0627\u0631\u064a\u0627\u062a \u0648\u062f\u064a\u0629 \u0627\u0654\u062e\u0631\u0649\u062e\u0633\u0631 \u0627\u0644\u0647\u0644\u0627\u0644 \u0627\u0654\u0645\u0627\u0645 \u0627\u0655\u0646\u062a\u0631 \u0645\u064a\u0644\u0627\u0646 \u0628\u0646\u062a\u064a\u062c\u0629 1-0 \u0641\u064a 2010.\u062e\u0633\u0631 \u0627\u0654\u0645\u0627\u0645 \u064a\u0648\u0641\u0646\u062a\u0648\u0633 7-1 \u0641\u064a 2012.\u0641\u0627\u0632 \u0639\u0644\u0649 \u062f\u064a\u0646\u0627\u0645\u0648 \u0643\u064a\u064a\u0641 3-1\u060c \u062b\u0645 \u062a\u0639\u0627\u062f\u0644 \u0633\u0644\u0628\u064a\u064b\u0627 \u0645\u0639 \u062c\u0646\u0648\u0649\u060c \u0648\u062e\u0633\u0631 \u0627\u0654\u0645\u0627\u0645 \u0627\u0654\u062a\u0644\u062a\u064a\u0643 \u0628\u064a\u0644\u0628\u0627\u0648 3-0 \u0641\u064a 2014.\u062e\u0633\u0631 \u0627\u0654\u0645\u0627\u0645 \u0628\u0627\u064a\u0631\u0646 \u0645\u064a\u0648\u0646\u062e 4-1 \u0641\u064a 2015.\u062e\u0633\u0631 \u0627\u0654\u0645\u0627\u0645 \u062a\u0634\u064a\u0644\u0633\u064a 1-0 \u0641\u064a 2022.\u0641\u0627\u0632 \u0639\u0644\u0649 \u0646\u0648\u062a\u0646\u062c\u0647\u0627\u0645 \u0641\u0648\u0631\u0633\u062a 6-1\u060c \u0648\u062a\u063a\u0644\u0628 \u0639\u0644\u0649 \u0627\u0654\u0644\u0645\u064a\u0631\u064a\u0627 2-1 \u0641\u064a \u0646\u0641\u0633 \u0627\u0644\u0639\u0627\u0645.\u062e\u0633\u0631 \u0627\u0654\u0645\u0627\u0645 \u062f\u064a\u0646\u0627\u0645\u0648 \u0643\u064a\u064a\u0641 1-0 \u0641\u064a 2023.\u062a\u0639\u0627\u062f\u0644 \u0645\u0639 \u0631\u064a\u0627\u0644 \u0645\u062f\u0631\u064a\u062f 1-1 \u0641\u064a 2025.\u062a\u0639\u0627\u062f\u0644 \u0645\u0639 \u0633\u0627\u0644\u0632\u0628\u0648\u0631\u063a 0-0 \u0641\u064a 2025.\u0627\u0644\u0647\u0644\u0627\u0644 \u0627\u0644\u064a\u0648\u0645: \u0637\u0645\u0648\u062d \u0639\u0627\u0644\u0645\u064a \u0648\u0627\u0654\u0633\u0645\u0627\u0621 \u0643\u0628\u064a\u0631\u0629\u0627\u0654\u0635\u0628\u062d \u0627\u0644\u0647\u0644\u0627\u0644 \u0641\u0631\u064a\u0642\u064b\u0627 \u0639\u0627\u0644\u0645\u064a\u064b\u0627\u060c \u064a\u0645\u062a\u0644\u0643 \u0627\u0644\u0627\u0655\u0645\u0643\u0627\u0646\u064a\u0627\u062a \u0648\u0627\u0644\u0637\u0645\u0648\u062d \u0644\u0645\u0648\u0627\u062c\u0647\u0629 \u0627\u0654\u0642\u0648\u0649 \u0627\u0644\u0627\u0654\u0646\u062f\u064a\u0629\u060c \u0645\u0633\u062a\u0641\u064a\u062f\u064b\u0627 \u0645\u0646 \u062f\u0639\u0645 \u0635\u0641\u0648\u0641\u0647 \u0628\u0646\u062c\u0648\u0645 \u0639\u0627\u0644\u0645\u064a\u064a\u0646 \u0645\u062b\u0644 \u0627\u0654\u0644\u0643\u0633\u0646\u062f\u0631 \u0645\u064a\u062a\u0631\u0648\u0641\u064a\u062a\u0634\u060c \u0643\u0627\u0644\u064a\u062f\u0648 \u0643\u0648\u0644\u064a\u0628\u0627\u0644\u064a\u060c \u0633\u064a\u0631\u062c\u064a \u0645\u064a\u0644\u064a\u0646\u0643\u0648\u0641\u064a\u062a\u0634-\u0633\u0627\u0641\u064a\u062a\u0634 \u0648\u0645\u0627\u0644\u0643\u0648\u0645\u060c \u0628\u0627\u0644\u0627\u0655\u0636\u0627\u0641\u0629 \u0627\u0655\u0644\u0649 \u0644\u0627\u0639\u0628\u064a\u0646 \u0633\u0639\u0648\u062f\u064a\u064a\u0646 \u0628\u0627\u0631\u0632\u064a\u0646 \u0645\u062b\u0644 \u0633\u0627\u0644\u0645 \u0627\u0644\u062f\u0648\u0633\u0631\u064a\u060c \u062d\u0633\u0627\u0646 \u062a\u0645\u0628\u0643\u062a\u064a \u0648\u0645\u062d\u0645\u062f \u0643\u0646\u0648.\u0645\u0646 \u0627\u0644\u0641\u0648\u0632 \u0639\u0644\u0649 \u0641\u0627\u0644\u0646\u0633\u064a\u0627\u060c \u0627\u0655\u0644\u0649 \u0627\u0655\u0633\u0642\u0627\u0637 \u0645\u0627\u0646\u0634\u0633\u062a\u0631 \u064a\u0648\u0646\u0627\u064a\u062a\u062f\u060c \u0648\u0635\u0648\u0644\u064b\u0627 \u0627\u0655\u0644\u0649 \u0627\u0644\u062a\u0641\u0648\u0642 \u0639\u0644\u0649 \u0627\u0654\u0648\u062f\u064a\u0646\u064a\u0632\u064a\u060c \u064a\u064f\u0638\u0647\u0631 \u0627\u0644\u0647\u0644\u0627\u0644 \u0634\u062e\u0635\u064a\u0629 \u0627\u0644\u0641\u0631\u064a\u0642 \u0627\u0644\u0630\u064a \u0644\u0627 \u064a\u062e\u0634\u0649 \u0627\u0644\u062a\u062d\u062f\u064a\u0627\u062a. \u0648\u0645\u0639 \u0627\u0642\u062a\u0631\u0627\u0628 \u0645\u0648\u0627\u062c\u0647\u0629 \u0633\u0627\u0644\u0632\u0628\u0648\u0631\u063a \u0641\u064a \u0643\u0627\u0654\u0633 \u0627\u0644\u0639\u0627\u0644\u0645 \u0644\u0644\u0627\u0654\u0646\u062f\u064a\u0629\u060c \u062a\u062a\u062c\u062f\u062f \u0627\u0644\u0641\u0631\u0635\u0629 \u0627\u0654\u0645\u0627\u0645 \"\u0627\u0644\u0632\u0639\u064a\u0645\" \u0644\u0643\u062a\u0627\u0628\u0629 \u0641\u0635\u0644 \u062c\u062f\u064a\u062f \u0641\u064a \u0633\u062c\u0644\u0647 \u0627\u0644\u0645\u0634\u0631\u0641\u060c \u0648\u062a\u0627\u0654\u0643\u064a\u062f \u0642\u062f\u0631\u0629 \u0627\u0644\u0643\u0631\u0629 \u0627\u0644\u0633\u0639\u0648\u062f\u064a\u0629 \u0639\u0644\u0649 \u0645\u062c\u0627\u0631\u0627\u0629 \u0643\u0628\u0627\u0631 \u0627\u0644\u0627\u0654\u0646\u062f\u064a\u0629 \u0627\u0644\u0639\u0627\u0644\u0645\u064a\u0629.\u0633\u062c\u0644 \u0645\u0648\u0627\u062c\u0647\u0627\u062a \u0627\u0644\u0647\u0644\u0627\u0644 \u0636\u062f \u0627\u0644\u0627\u0654\u0646\u062f