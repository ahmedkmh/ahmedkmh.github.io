ywords": null, "creator": ["\u0627\u0644\u064a\u0648\u0645- \u0644\u0646\u062f\u0646"], "description": "\u064a\u062e\u0648\u0636 \u0645\u0627\u0646\u0634\u0633\u062a\u0631 \u064a\u0648\u0646\u0627\u064a\u062a\u062f \u0627\u0644\u0627\u0655\u0646\u0643\u0644\u064a\u0632\u064a \u0645\u0628\u0627\u0631\u062a\u064a\u0646 \u0648\u062f\u064a\u062a\u064a\u0646 \u0641\u064a \u0645\u0627\u0644\u064a\u0632\u064a\u0627 \u0648\u0647\u0648\u0646\u063a \u0643\u0648\u0646\u063a \u0628\u0639\u062f \u062b\u0644\u0627\u062b\u0629 \u0627\u0654\u064a\u0627\u0645 \u0645\u0646 \u0646\u0647\u0627\u064a\u0629 \u0645\u0648\u0633\u0645 \u0627\u0644\u062f\u0648\u0631\u064a \u0627\u0644\u0627\u0655\u0646\u0643\u0644\u064a\u0632\u064a \u0641\u064a \u0643\u0631\u0629 \u0627\u0644\u0642\u062f\u0645\u060c \u0643\u0645\u0627 \u0627\u0654\u0639\u0644\u0646 \u0627\u0644\u0646\u0627\u062f\u064a \u0627\u0644\u0634\u0645\u0627\u0644\u064a \u0627\u0644\u0639\u0631\u064a\u0642 \u0627\u0644\u062b\u0644\u0627\u062b\u0627\u0621.\u0648\u0633\u062a\u062e\u0648\u0636 \u0643\u062a\u064a\u0628\u0629 \u0627\u0644\u0645\u062f\u0631\u0628 \u0627\u0644\u0628\u0631\u062a\u063a\u0627\u0644\u064a \u0631\u0648\u0628\u0646 \u0627\u0654\u0645\u0648\u0631\u064a\u0645 \u0645\u0648\u0627\u062c\u0647\u0629 \u0636\u062f \u0646\u062c\u0648\u0645 \u0645\u0646\u0637\u0642\u0629 \u0627\u0653\u0633\u064a\u0627\u0646 \u0641\u064a \u0643\u0648\u0627\u0644\u0627\u0644\u0645\u0628\u0648\u0631 \u0641\u064a 28 \u0627\u0654\u064a\u0627\u0631/\u0645\u0627\u064a\u0648 \u0648\u062a\u0634\u0643\u064a\u0644\u0629 \u0645\u0646 \u0627\u0654\u0646\u062f\u064a\u0629 \u0647\u0648\u0646\u063a \u0643\u0648\u0646\u063a \u0641\u064a 30 \u0645\u0646\u0647.\u0648\u064a\u0646\u0647\u064a \u0645\u0627\u0646\u0634\u0633\u062a\u0631 \u064a\u0648\u0646\u0627\u064a\u062a\u062f \u0627\u0644\u062f\u0648\u0631\u064a \u0627\u0644\u0627\u0655\u0646\u0643\u0644\u064a\u0632\u064a \u0628\u0645\u0648\u0627\u062c\u0647\u0629 \u0627\u0654\u0633\u062a\u0648\u0646 \u0641\u0