u0631\u062f\u0647 \u0627\u0644\u0645\u0628\u0627\u0634\u0631 \u0631\u064a\u0627\u0644 \u0645\u062f\u0631\u064a\u062f \u062d\u0627\u0645\u0644 \u0627\u0644\u0644\u0642\u0628 \u0627\u0644\u0633\u0628\u062a \u0641\u064a \u0627\u0644\u0645\u0631\u062d\u0644\u0629 \u0627\u0644\u062b\u0627\u0646\u064a\u0629 \u0648\u0627\u0644\u062b\u0644\u0627\u062b\u064a\u0646 \u0645\u0646 \u0627\u0644\u062f\u0648\u0631\u064a \u0627\u0644\u0627\u0655\u0633\u0628\u0627\u0646\u064a \u0644\u0643\u0631\u0629 \u0627\u0644\u0642\u062f\u0645.\u0648\u0627\u0654\u0647\u062f\u0649 \u0627\u0644\u0628\u0631\u0627\u0632\u064a\u0644\u064a \u0631\u0627\u0641\u064a\u0646\u064a\u0627 \u0627\u0644\u0646\u0642\u0627\u0637 \u0627\u0644\u062b\u0644\u0627\u062b \u0627\u0644\u062b\u0645\u064a\u0646\u0629 \u0644\u0641\u0631\u064a\u0642\u0647 \u0628\u0639\u062f\u0645\u0627 \u0627\u062e\u062a\u062a\u0645 \u0645\u0647\u0631\u062c\u0627\u0646 \u0627\u0644\u0627\u0654\u0647\u062f\u0627\u0641 \u0645\u0646 \u0631\u0643\u0644\u0629 \u062c\u0632\u0627\u0621 \u0641\u064a \u0627\u0644\u062f\u0642\u064a\u0642\u0629 \u0627\u0644\u062b\u0627\u0645\u0646\u0629 \u0645\u0646 \u0627\u0644\u0648\u0642\u062a \u0628\u062f\u0644 \u0627\u0644\u0636\u0627\u064a\u0654\u0639 \u0627\u0654\u0643\u062f\u0647\u0627 \u062d\u0643\u0645 \u0627\u0644\u0641\u064a\u062f\u064a\u0648 \u0627\u0644\u0645\u0633\u0627\u0639\u062f \"\u0641\u064a \u0627\u064a\u0647 \u0627\u0653\u0631\"\u060c \u0628\u0639\u062f\u0645\u0627 \u0643\u0627\u0646 \u0627\u0654\u062f\u0631\u0643 \u0627\u0644\u062a\u0639\u0627\u062f\u0644 3-3 \u0641\u064a \u0627\u0644\u062f\u0642\u064a\u0642\u0629 68\u060c \u0639\u0642\u0628 \u0647\u062f\u0641\u064a \u0641\u064a\u0631\u0627\u0646 \u062a\u0648\u0631\u064a\u0633 \u0627\u0644\u0630\u064a \u0645\u0646\u062d \u0627\u0644\u0627\u0654\u0641\u0636\u0644\u064a\u0629 \u0644\u0628\u0631\u0634\u0644\u0648\u0646\u0629 (12) \u0648\u0627\u0644\u0628\u062f\u064a\u0644 \u062f\u0627\u0646\u064a \u0627\u0654\u0648\u0644\u0645\u0648 (65)\u060c \u0631\u062f\u0627 \u0639\u0644\u0649 \u062b\u0644\u0627\u062b\u064a\u0629 \"\u0647\u0627\u062a\u0631\u064a\u0643\" \u0628\u0648\u0631\u062e\u0627 \u0627\u0655\u064a\u063a\u0644\u064a\u0633\u064a\u0627\u0633 \u0644\u0635\u0627\u0644\u062d \u0633\u0644\u062a\u0627 \u0641\u064a\u063a\u0648 (15 \u064852 \u064862).-\"\u0643\u0646\u0627 \u0646\u0633\u062a\u062d\u0642 \u0627\u0644\u0641\u0648\u0632\"-\u0642\u0627\u0644 \u0627\u0654\u0648\u0644\u0645\u0648 \u0627\u0644\u0630\u064a \u062f\u062e\u0644 \u0641\u064a \u0627\u0644\u062f\u0642\u064a\u0642\u0629 60 \u0628\u062f\u0644\u0627 \u0645\u0646 \u062a\u0648\u0631\u064a\u0633 \u0648\u0633\u062c\u0644 \u0647\u062f\u0641\u0627 \u0648\u062a\u0633\u0628\u0628 \u0628\u0631\u0643\u0644\u0629 \u0627\u0644\u062c\u0632\u0627\u0621 \u0627\u0644\u062d\u0627\u0633\u0645\u0629 \u0628\u0639\u062f \u062e\u0637\u0627\u0654 \u0639\u0644\u064a\u0647 \u0645\u0646 \u0627\u0644\u062e\u0644\u0641 \u0645\u0646 \u0642\u0628\u0644 \u0627\u0644\u0634\u0627\u0628 \u0648\u064a\u0644 \u0644\u0627\u063a\u0648 (21 \u0639\u0627\u0645\u0627) \u0644\u0645\u0646\u0635\u0629 \"\u062f\u0627\u0632\u0648\u0646\" \u0644\u0644\u0628\u062b \u0627\u0644\u062a\u062f\u0641\u0642\u064a \"\u0647\u0630\u0647 \u0647\u064a \u0643\u0631\u0629 \u0627\u0644\u0642\u062f\u0645\u060c \u0643\u0646\u0627 \u0646\u0631\u063a\u0628 \u0628\u0634\u062f\u0629 \u0641\u064a \u0627\u0644\u0641\u0648\u0632\u060c \u0631\u0628\u0645\u0627 \u0644\u0645 \u0646\u0643\u0646 \u0641\u064a \u0627\u0654\u0641\u0636\u0644 \u062d\u0627\u0644\u0627\u062a\u0646\u0627 (\u0641\u064a \u0628\u062f\u0627\u064a\u0629 \u0627\u0644\u0645\u0628\u0627\u0631\u0627\u0629)\u060c \u0644\u0643\u0646 \u0631\u062f \u0641\u0639\u0644 \u0627\u0644\u0641\u0631\u064a\u0642 \u0648\u062a\u0634\u062c\u064a\u0639 \u0627\u0644\u062c\u0645\u0627\u0647\u064a\u0631 \u0644\u0646\u0627... \u0643\u0646\u0627 \u0646\u0633\u062a\u062d\u0642 \u0627\u0644\u0641\u0648\u0632 \u0628\u062c\u062f\u0627\u0631\u0629\".\u0648\u0627\u0654\u0636\u0627\u0641 \u0628\u0637\u0644 \u0643\u0627\u0654\u0633 \u0627\u0654\u0648\u0631\u0648\u0628\u0627 \u0645\u0639 \u0645\u0646\u062a\u062e\u0628 \"\u0644\u0627 \u0631\u0648\u062e\u0627\": \"\u0647\u0630\u0647 \u062b\u0644\u0627\u062b \u0646\u0642\u0627\u0637 \u062a\u064f\u0642\u0631\u0651\u0628\u0646\u0627 \u0645\u0646 \u0647\u062f\u0641\u0646\u0627\u060c \u0627\u0655\u0646\u0647\u0627 \u0645\u0647\u0645\u0629 \u062c\u062f\u0627 \u0648\u0639\u0644\u064a\u0646\u0627 \u0627\u0644\u0645\u0648\u0627\u0635\u0644\u0629 \u0639\u0644\u0649 \u0627\u0644\u0645\u0646\u0648\u0627\u0644 \u0630\u0627\u062a\u0647\".\u0648\u0631\u0641\u0639 \u0628\u0631\u0634\u0644\u0648\u0646\u0629 \u0631\u0635\u064a\u062f\u0647 \u0627\u0655\u0644\u0649 73 \u0646\u0642\u0637\u0629 \u0641\u064a \u0627\u0644\u0645\u0631\u0643\u0632 \u0627\u0644\u0627\u0654\u0648\u0644 \u0645\u062a\u0642\u062f\u0645\u0627 \u0628\u0641\u0627\u0631\u0642 7 \u0646\u0642\u0627\u0637 \u0639\u0646 \u0645\u0644\u0627\u062d\u0642\u0647 \u0631\u064a\u0627\u0644 \u0645\u062f\u0631\u064a\u062f \u0627\u0644\u0630\u064a \u064a\u0633\u062a\u0642\u0628\u0644 \u0641\u064a \u0645\u0648\u0627\u062c\u0647\u0629 \u0635\u0639\u0628\u0629 \u0627\u0654\u062a\u0644\u062a\u064a\u0643 \u0628\u0644\u0628\u0627\u0648 \u0627\u0644\u0627\u062d\u062f\u060c \u062d\u064a\u062b \u064a\u0627\u0654\u0645\u0644 \u0627\u0644\u0646\u0627\u062f\u064a \u0627\u0644\u0645\u0644\u0643\u064a \u0641\u064a \u062a\u0639\u0648\u064a\u0636 \u062e\u0631\u0648\u062c\u0647 \u0645\u0646 \u0645\u0633\u0627\u0628\u0642\u0629 \u062f\u0648\u0631\u064a \u0627\u0654\u0628\u0637\u0627\u0644 \u0627\u0654\u0648\u0631\u0648\u0628\u0627 \u0627\u0654\u0645\u0627\u0645 \u0627\u0654\u0631\u0633\u0646\u0627\u0644 \u0627\u0644\u0627\u0646\u0643\u0644\u064a\u0632\u064a \u0628\u0627\u0644\u0627\u062d\u062a\u0641\u0627\u0638 \u0628\u0644\u0642\u0628\u0647 \u0627\u0644\u0645\u062d\u0644\u064a\u060c \u0639\u0644\u0645\u0627 \u0627\u0654\u0646 \u0627\u0644\u0645\u0648\u0627\u062c\u0647\u0629 \u0628\u064a\u0646 \u0639\u0645\u0644\u0627\u0642\u064a \u0627\u0644\u0643\u0631\u0629 \u0627\u0644\u0627\u0655\u0633\u0628\u0627\u0646\u064a\u0629 \u0633\u062a\u062c\u062f\u062f \u0641\u064a \u0646\u0647\u0627\u064a\u0654\u064a \u0643\u0627\u0654\u0633 \u0627\u0644\u0645\u0644\u0643 \u0641\u064a 26 \u0627\u0644\u0634\u0647\u0631 \u0627\u0644\u062d\u0627\u0644\u064a \u0641\u064a \u0627\u0655\u0634\u0628\u064a\u0644\u064a\u0629.\u0648\u0628\u062e\u0644\u0627\u0641 \u0631\u064a\u0627\u0644\u060c \u062e\u0627\u0636 \u0628\u0631\u0634\u0644\u0648\u0646\u0629 \u0627\u0644\u0644\u0642\u0627\u0621 \u0645\u0646\u062a\u0634\u064a\u0627 \u0645\u0646 \u0628\u0644\u0648\u063a\u0647 \u0646\u0635\u0641 \u0646\u0647\u0627\u064a\u0654\u064a \u0627\u0644\u0645\u0633\u0627\u0628\u0642\u0629 \u0627\u0644\u0642\u0627\u0631\u064a\u0629 \u0627\u0644\u0627\u0654\u0645 \u0631\u063a\u0645 \u062e\u0633\u0627\u0631\u062a\u0647 \u0627\u0654\u0645\u0627\u0645 \u0628\u0648\u0631\u0648\u0633\u064a\u0627 \u062f\u0648\u0631\u062a\u0645\u0648\u0646\u062f \u0627\u0644\u0627\u0654\u0644\u0645\u0627\u0646\u064a 1-3 \u0627\u0655\u064a\u0627\u0628\u0627\u060c \u0645\u0633\u062a\u0641\u064a\u062f\u0627 \u0645\u0646 \u0641\u0648\u0632\u0647 \u0627\u0644\u0643\u0628\u064a\u0631 \u0630\u0647\u0627\u0628\u0627 \u0628\u0631\u0628\u0627\u0639\u064a\u0629 \u0646\u0638\u064a\u0641\u0629\u060c \u0644\u062a\u0646\u062a\u0647\u064a \u0633\u0644\u0633\u0644\u062a\u0647 \u0645\u0646 \u062f\u0648\u0646 \u062e\u0633\u0627\u0631\u0629 \u0641\u064a \u0645\u062e\u062a\u0644\u0641 \u0627\u0644\u0645\u0633\u0627\u0628\u0642\u0627\u062a \u0641\u064a \u0627\u0644\u0639\u0627\u0645 \u0627\u0644\u062d\u0627\u0644\u064a \u0639\u0646\u062f 20 \u0641\u0648\u0632\u0627 \u06484 \u062a\u0639\u0627\u062f\u0644\u0627\u062a.\u0648\u064a\u0633\u0639\u0649 \u0633\u0644\u062a\u0627 \u0641\u064a\u063a\u0648 \u0627\u0644\u0630\u064a \u062a\u062c\u0645\u062f \u0631\u0635\u064a\u062f\u0647 \u0639\u0646\u062f 43 \u0646\u0642\u0637\u0629 \u0641\u064a \u0627\u0644\u0645\u0631\u0643\u0632 \u0627\u0644\u0633\u0627\u0628\u0639 \u0641\u064a \u062d\u062c\u0632 \u0645\u0642\u0639\u062f \u0644\u0647 \u0641\u064a \u0645\u0633\u0627\u0628\u0642\u0629 \u0627\u0654\u0648\u0631\u0648\u0628\u064a\u0629 \u0627\u0644\u0645\u0648\u0633\u0645 \u0627\u0644\u0645\u0642\u0628\u0644\u060c \u0648\u0630\u0644\u0643 \u0644\u0644\u0645\u0631\u0629 \u0627\u0644\u0627\u0654\u0648\u0644\u0649 \u0645\u0646\u0630 \u0645\u0648\u0633\u0645 2016-2017 \u0639\u0646\u062f\u0645\u0627 \u0648\u0635\u0644 \u0627\u0655\u0644\u0649 \u0646\u0635\u0641 \u0646\u0647\u0627\u064a\u0654\u064a \u0645\u0633\u0627\u0628\u0642\u0629 \u0643\u0627\u0654\u0633 \u0627\u0644\u0627\u062a\u062d\u0627\u062f \u0627\u0644\u0627\u0648\u0631\u0648\u0628\u064a (\u064a\u0648\u0631\u0648\u0628\u0627 \u0644\u064a\u063a \u062d\u0627\u0644\u064a\u0627).\u0648\u064a\u0639\u062a\u0628\u0631 \u0633\u0644\u062a\u0627 \u0641\u064a\u063a\u0648 \u0628\u0639\u062f \u0631\u064a\u0627\u0644 \u0645\u062f\u0631\u064a\u062f \u0627\u0644\u0641\u0631\u064a\u0642 \u0627\u0644\u0627\u0654\u0642\u0644 \u062e\u0633\u0627\u0631\u0629 \u0627\u0654\u0645\u0627\u0645 \u0628\u0631\u0634\u0644\u0648\u0646\u0629 \u0645\u0646\u0630 \u0628\u062f\u0627\u064a\u0629 \u0645\u0648\u0633\u0645 2017-2018 (8 \u0647\u0632\u0627\u064a\u0654\u0645)\u060c \u0639\u0644\u0645\u0627 \u0627\u0646\u0647 \u0646\u062c\u062d \u0641\u064a \u0645\u0628\u0627\u0631\u064a\u0627\u062a\u0647 \u0627\u0644\u0640 16 \u0627\u0644\u0627\u0654\u062e\u064a\u0631\u0629 \u0627\u0654\u0645\u0627\u0645 \u0627\u0644\u0646\u0627\u062f\u064a \u0627\u0644\u0643\u0627\u062a\u0627\u0644\u0648\u0646\u064a \u0641\u064a \u0627\u0644\u0641\u0648\u0632 \u0641\u064a \u062b\u0644\u0627\u062b \u0645\u0646\u0647\u0627\u060c \u0645\u0642\u0627\u0628\u0644 5 \u062a\u0639\u0627\u062f\u0644\u0627\u062a.-\u0627\u0655\u0635\u0627\u0628\u0629 \u0644\u064a\u0641\u0627\u0646\u062f\u0648\u0641\u0633\u0643\u064a \u0648\"\u062a\u0641\u0627\u0635\u064a\u0644 \u0635\u063a\u064