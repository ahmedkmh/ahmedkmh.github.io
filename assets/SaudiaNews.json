u0628\u0627\u062f\u0644 \u0627\u0644\u0639\u0645\u0644\u0627\u062a \u0627\u0644\u0631\u0642\u0645\u064a\u0629\u060c \u0639\u0646 \u062a\u0643\u0647\u0646\u0647 \u0628\u0627\u0654\u0646 \u0639\u0645\u0644\u064a\u0629 \u0646\u0642\u0644 8.6 \u0645\u0644\u064a\u0627\u0631 \u062f\u0648\u0644\u0627\u0631 \u0645\u0646 \u0639\u0645\u0644\u0629 \u0627\u0644\u0628\u064a\u062a\u0643\u0648\u064a\u0646 \u0645\u0646 \u062b\u0645\u0627\u0646\u064a \u0645\u062d\u0627\u0641\u0638 \u0643\u0627\u0646\u062a \u062e\u0627\u0645\u0644\u0629 \u0644\u0627\u0654\u0643\u062b\u0631 \u0645\u0646 14 \u0639\u0627\u0645\u064b\u0627\u060c \u0642\u062f \u062a\u0642\u0641 \u0648\u0631\u0627\u064a\u0654\u0647\u0627 \u0639\u0645\u0644\u064a\u0629 \u0627\u062e\u062a\u0631\u0627\u0642 \u0627\u0655\u0644\u0643\u062a\u0631\u0648\u0646\u064a.\u0648\u0642\u0627\u0644 \u062c\u0631\u0648\u062c\u0627\u0646 \u0641\u064a \u0645\u0646\u0634\u0648\u0631 \u0639\u0644\u0649 \"\u0627\u0655\u0643\u0633\"\u060c \u064a\u0648\u0645 \u0627\u0644\u062c\u0645\u0639\u0629\u060c: \"\u0627\u0655\u0630\u0627 \u0635\u062d\u0651 \u062a\u0648\u0642\u0639\u064a \u0647\u0630\u0627\u060c \u0631\u063a\u0645 \u0636\u0627\u0653\u0644\u0629 \u0627\u062d\u062a\u0645\u0627\u0644\u0647\u060c \u0641\u0633\u062a\u0643\u0648\u0646 \u0647\u0630\u0647 \u0627\u0654\u0643\u0628\u0631 \u0639\u0645\u0644\u064a\u0629 \u0633\u0631\u0642\u0629 \u0641\u064a \u062a\u0627\u0631\u064a\u062e \u0627\u0644\u0628\u0634\u0631\u064a\u0629\".\u0648\u0627\u0654\u0636\u0627\u0641 \u062c\u0631\u0648\u062c\u0627\u0646: \"\u0647\u0646\u0627\u0643 \u0627\u062d\u062a\u0645\u0627\u0644 \u0636\u064a\u0654\u064a\u0644 \u0628\u0627\u0654\u0646 \u062a\u0643\u0648\u0646 \u0639\u0645\u0644\u064a\u0629 \u0627\u0644\u062a\u062d\u0648\u064a\u0644 \u0627\u0644\u0628\u0627\u0644\u063a\u0629 8 \u0645\u0644\u064a\u0627\u0631\u0627\u062a \u062f\u0648\u0644\u0627\u0631 \u064a\u0648\u0645 \u0627\u0644\u062e\u0645\u064a\u0633 \u0627\u0644\u0645\u0627\u0636\u064a\u060c \u0643\u0627\u0646\u062a \u0646\u062a\u064a\u062c\u0629 \u0644\u0644\u0627\u062e\u062a\u0631\u0627\u0642 \u0627\u0654\u0648 \u062a\u0645 \u0627\u062e\u062a\u0631\u0627\u0642 \u0627\u0644\u0645\u0641\u0627\u062a\u064a\u062d \u0627\u0644\u062e\u0627\u0635\u0629\"\u060c \u0645\u0634\u064a\u0631\u064b\u0627 \u0627\u0655\u0644\u0649 \u0645\u0639\u0627\u0645\u0644\u0629 \u0645\u0634\u0628\u0648\u0647\u0629 \u0628\u0642\u064a\u0645\u0629 485.29 \u062f\u0648\u0644\u0627\u0631\u064b\u0627 \u0627\u0654\u0645\u0631\u064a\u0643\u064a\u064b\u0627 \u0645\u0646 \u0628\u064a\u062a\u0643\u0648\u064a\u0646 \u0643\u0627\u0634 \u062d\u062f\u062b\u062a \u0642\u0628\u0644 \u0627\u0644\u062a\u062d\u0648\u064a\u0644\u0627\u062a \u0627\u0644\u0643\u0628\u064a\u0631\u0629 \u064a\u0648\u0645 \u0627\u0644\u062e\u0645\u064a\u0633 \u0648\u0627\u0644\u062a\u064a \u0634\u0645\u0644\u062a 10,000 \u0628\u064a\u062a\u0643\u0648\u064a\u0646 \u0641\u064a \u0627\u0644\u0645\u0631\u0629 \u0627\u0644\u0648\u0627\u062d\u062f\u0629.\u0648\u0642\u0627\u0644: \u0639\u062b\u0631\u062a\u064f \u0639\u0644\u0649 \u0645\u0639\u0627\u0645\u0644\u0629 \u062a\u062c\u0631\u064a\u0628\u064a\u0629 \u0648\u0627\u062d\u062f\u0629 \u0644\u0639\u0645\u0644\u0629 \u0628\u064a\u062a\u0643\u0648\u064a\u0646 \u0643\u0627\u0634 \u0645\u0646 \u0627\u0655\u062d\u062f\u0649 \u0645\u062c\u0645\u0648\u0639\u0627\u062a \u0627\u0644\u062d\u064a\u062a\u0627\u0646 \u0642\u0628\u0644 14 \u0633\u0627\u0639\u0629\u060c \u0645\u062a\u0628\u0648\u0639\u0629\u064b \u0628\u0627\u0644\u0645\u0628\u0644\u063a \u0643\u0627\u0645\u0644\u0627\u064b. \u0648\u0628\u0639\u062f \u0633\u0627\u0639\u0629\u060c \u0628\u062f\u0627\u0654\u062a \u062a\u0644\u0643 \u0627\u0644\u0645\u062d\u0627\u0641\u0638 \u0641\u064a \u0627\u0644\u0646\u0634\u0627\u0637.\"\u0648\u0639\u0644\u0649 \u0627\u0644\u0631\u063a\u0645 \u0645\u0646 \u062d\u0627\u0644\u0629 \u0627\u0644\u063a\u0645\u0648\u0636\u060c \u0638\u0644 \u0633\u0639\u0631 \u0627\u0644\u0628\u064a\u062a\u0643\u0648\u064a\u0646 \u0645\u0633\u062a\u0642\u0631\u064b\u0627 \u0646\u0633\u0628\u064a\u064b\u0627\u060c \u062d\u064a\u062b \u0627\u0646\u062e\u0641\u0636 \u0628\u0646\u0633\u0628\u0629 1.02% \u062e\u0644\u0627\u0644 \u0627\u0644\u064024 \u0633\u0627\u0639\u0629 \u0627\u0644\u0645\u0627\u0636\u064a\u0629\u060c \u0648\u064a\u062a\u062f\u0627\u0648\u0644 \u0639\u0646\u062f 108,150 \u062f\u0648\u0644\u0627\u0631\u064b\u0627 \u0627\u0644\u064a\u0648\u0645 \u0627\u0644\u0627\u0654\u062d\u062f\u060c \u0648\u0641\u0642\u064b\u0627 \u0644\u0645\u0648\u0642\u0639 \"\u0643\u0648\u064a\u0646 \u0645\u0627\u0631\u0643\u062a \u0643\u0627\u0628\".\u0648\u0627\u0654\u0648\u0636\u062d \u062c\u0631\u0648\u062c\u0627\u0646 \u0627\u0654\u0646\u0647 \u0645\u0646 \u0627\u0644\u0645\u062d\u062a\u0645\u0644 \u0627\u0654\u0646 \u064a\u0643\u0648\u0646 \u0645\u0627\u0644\u0643 \u0627\u0644\u0645\u062d\u0641\u0638\u0629 \u0642\u062f \u0627\u062e\u062a\u0628\u0631 \u0627\u0644\u0645\u0641\u062a\u0627\u062d \u0627\u0644\u062e\u0627\u0635 \u0633\u0631\u064b\u0627\u060c \u0644\u0627\u0654\u0646 \u0645\u0639\u0627\u0645\u0644\u0627\u062a \u0628\u064a\u062a\u0643\u0648\u064a\u0646 \u0643\u0627\u0634 \u0644\u0627 \u062a\u062c\u0630\u0628 \u0627\u0647\u062a\u0645\u0627\u0645\u064b\u0627 \u0643\u0628\u064a\u0631\u064b\u0627 \u0645\u0646 \u062e\u062f\u0645\u0627\u062a \u062a\u062a\u0628\u0639 \u0627\u0644\u062d\u064a\u062a\u0627\u0646. \u0648\u0645\u0639 \u0630\u0644\u0643\u060c \u0627\u0654\u0643\u062f \u0645\u062c\u062f\u062f\u064b\u0627 \u0627\u0654\u0646 \u0647\u0630\u0627 \u0627\u0644\u0633\u0644\u0648\u0643 \u0644\u0627 \u064a\u0639\u062f \u0645\u0627\u0654\u0644\u0648\u0641\u064b\u0627.\u0648\u0642\u0627\u0644\u062a \u0634\u0631\u0643\u0629 \"\u0627\u0654\u0631\u0643\u0627\u0645\" \u0627\u0644\u0645\u062a\u062e\u0635\u0635\u0629 \u0641\u064a \u062a\u062a\u0628\u0639 \u0627\u0644\u0639\u0645\u0644\u0627\u062a \u0627\u0644\u0631\u0642\u0645\u064a\u0629 \u0648\u0627\u0633\u062a\u062e\u0628\u0627\u0631\u0627\u062a \u06